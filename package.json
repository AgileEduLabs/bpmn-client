{
  "name": "bpmn-client",
  "version": "1.3.8",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "compileRun": "tsc --build && node cli.js",
    "dev": "nodemon -e ts  --exec \"npm run compileRun\"",
    "clean": "tsc --build --clean",
    "start": "node cli.ts",
    "test": "mocha -exit --config=test\\.mocharc.json"
  },
  "description": "Client API for BPMN-Server webservices",
  "keywords": [
    "BPMN",
    "BPMN 2.0",
    "Workflow",
    "Node.js",
    "TypeScript"
  ],
  "author": {
    "name": "ralphhanna"
  },
  "repository": {
    "type": "git",
    "url": "git+https:github.com/ralphhanna/bpmn-client.git"
  },
  "dependencies": {
    "connect-busboy": "0.0.2",
    "core-js": "^3.6.5",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@types/mime": "^1.3.1",
    "@types/node": "^8.10.62",
    "@types/serve-static": "^1.7.32",
    "nodemon": "^2.0.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=12.0.0"

  }
}
