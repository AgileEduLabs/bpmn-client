{
  "name": "bpmn-client",
  "version": "1.0.3",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start:watch": "nodemon app",
    "start": "node app.ts",
    "test": "mocha -R dot",
    "posttest": "eslint . --cache && npm run toc",
    "wintest": "node_modules/.bin/mocha",
    "toc": "node scripts/generate-api-toc ./docs/API.md,./docs/Examples.md",
    "test-md": "node scripts/test-markdown.js ./docs/API.md && node scripts/test-markdown.js ./docs/Examples.md"
  },
  "description": "bpmn-server.webApp",
  "keywords": [
    "BPMN",
    "BPMN 2.0",
    "Workflow",
    "Node.js",
    "TypeScript"
  ],
  "author": {
    "name": "ralphhanna"
  },
  "repository": {
    "type": "git",
    "url": "git+https:github.com/ralphhanna/bpmn-server.git"
  },
  "dependencies": {
    "bpmn-server": "^1.0.0",
    "connect-busboy": "0.0.2",
    "core-js": "^3.6.5",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.1"
  },
  "devDependencies": {
    "@types/mime": "^1.3.1",
    "@types/node": "^8.10.62",
    "@types/serve-static": "^1.7.32",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": "^8.0.0"
  }
}
